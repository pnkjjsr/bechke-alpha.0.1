{
  "name": "bechke",
  "private": true,
  "version": "0.0.1",
  "description": "Bechke App",
  "engines": {
    "node": "12"
  },
  "main": "server.js",
  "scripts": {
    "dev": "next dev src/",
    "build": "next build src/",
    "start": "next start src/",
    "lint": "next lint src/",
    "serve": "npm run build && firebase emulators:start --only functions,hosting",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions,hosting",
    "cleandeploy": "yarn clean && yarn rmnode && yarn deploy",
    "logs": "firebase functions:log",
    "clean": "rimraf .next && rimraf package-lock.json && rimraf .firebase && rimraf firebase-debug.log && rimraf yarn.lock && rimraf ui-debug.log && rimraf public/cache",
    "rmnode": "rimraf node_modules"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.0",
    "@chakra-ui/react": "^1.4.2",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "framer-motion": "^4.0.3",
    "firebase-admin": "^10.0.0",
    "firebase-functions": "^3.16.0",
    "next": "latest",
    "next-pwa": "^3.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "webpack": "^5.62.2"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "eslint": "7",
    "eslint-config-next": "12.0.3",
    "prop-types": "latest",
    "rimraf": "^3.0.2",
    "typescript": "4.4.4"
  }
}
